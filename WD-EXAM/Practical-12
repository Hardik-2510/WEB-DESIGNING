        // Date in Js
        const d = new Date("2023-10-25");
        const date = document.getElementById("DateDemo").innerHTML = d;

        // Object in JS
        const human = {name:"Hardik" , age: 19 , city:"Surat"}
        document.getElementById("ObjectValue").innerText = human.name + " " +human.age ;

        // Array in JS
        const fruits = [" Apple " , " Banana " , " Mango "]
        document.getElementById("ArrayDemo").innerHTML = fruits;

        // String in JS
        const str = `This is a Example of String , This Array Elements \"${fruits}\" , This is length of String :- ` ;
        const strL = str.length;
        document.getElementById("stringDemo").innerHTML = str + " " + strL;


        // PopBoxes in Js
        alert("Hello From Deveopers Community !");
